create schema customer_database;

CREATE TABLE IF NOT EXISTS customer_database.customer(
id INT NOT NULL auto_increment,
firstname VARCHAR(255) NOT NULL,
lastname VARCHAR(255) NOT NULL,
email varchar (20) NOT NULL,
PRIMARY KEY (id));

